<?php
$this->navActive = 'options';
function walk ($a = null, $b = null, $prefix = null)
{
	if (is_array($b)) {
		foreach ($b as $c => $d) {
			if (! is_null($prefix))
				walk($c, $d, "$prefix.$a");
			elseif (! is_null($a))
				walk($c, $d, "$a");
			else
				walk($c, $d);
		}
	} else {
		echo "$prefix.$a\t$b\n";
	}
}
?>
<pre>
<?php
walk(null, $this->appConfig);
?>
</pre>